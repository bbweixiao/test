<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  
  <servlet>
  	<servlet-name>app</servlet-name>
  	<servlet-class>com.kaishengit.web.AppServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>com.kaishengit.web.MainServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AccountServlet</servlet-name>
    <servlet-class>com.kaishengit.web.AccountServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RecodeServlet</servlet-name>
    <servlet-class>com.kaishengit.web.RecodeServlet</servlet-class>
  </servlet>



  <servlet-mapping>
  	<servlet-name>app</servlet-name>
  	<url-pattern>/index.jspx</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/main.jspx</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AccountServlet</servlet-name>
    <url-pattern>/account.jspx</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RecodeServlet</servlet-name>
    <url-pattern>/recode.jspx</url-pattern>
  </servlet-mapping>
  
  	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
